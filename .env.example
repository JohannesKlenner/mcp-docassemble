# Docassemble MCP Server Configuration
# 
# Copy this file to .env and fill in your Docassemble instance details
# 
# REQUIRED: Docassemble Server Base URL
DOCASSEMBLE_BASE_URL=https://your-docassemble-server.com

# REQUIRED: Your Docassemble API Key
# Get this from: My Account > API Keys in your Docassemble installation
DOCASSEMBLE_API_KEY=your_api_key_here

# OPTIONAL: Logging level (DEBUG, INFO, WARNING, ERROR)
# LOG_LEVEL=INFO

# OPTIONAL: Request timeout in seconds  
# REQUEST_TIMEOUT=30

# OPTIONAL: Enable verbose logging for debugging
# VERBOSE=false
